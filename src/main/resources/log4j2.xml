<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
  <Appenders>
   
    <Console name="STDOUT" target="SYSTEM_OUT">
      <PatternLayout pattern="%d %-5p [%t] %c{5} (%F:%L) - %m%n"/>
    </Console>
  </Appenders>
  
    <Root level="info">
      <AppenderRef ref="STDOUT"/>
    </Root>
</Configuration>